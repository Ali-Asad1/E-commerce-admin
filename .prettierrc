{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "printWidth": 120,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/lib(?:/.*)?$",
    "^@/providers(?:/.*)?$",
    "^@/services(?:/.*)?$",
    "^@/types(?:/.*)?$",
    "^@/hooks(?:/.*)?$",
    "^@/utils(?:/.*)?$",
    "^@/components(?:/.*)?$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false
}
